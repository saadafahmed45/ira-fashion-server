{
  "version": 2,

  "builds": [{ "src": "./index.js", "use": "@vercel/node" }],
  "routes": [
    {
      "source": "/(.*)",

      "destination": "/",
      "methods": ["GET", "POST", "PUT", "PATCH", "DELETE","OPTION"]
    }
  ],

  "rewrites": [
    {
      "source": "/(.*)",

      "destination": "/api"
    }
  ]
}
